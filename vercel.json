{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/styles.css",
      "dest": "/server.js"
    },
    {
      "src": "/script.js",
      "dest": "/server.js"
    },
    {
      "src": "/config.js",
      "dest": "/server.js"
    },
    {
      "src": "/blossompix.png",
      "dest": "/server.js"
    },
    {
      "src": "/twitter button.png",
      "dest": "/server.js"
    },
    {
      "src": "/Github button.png",
      "dest": "/server.js"
    },
    {
      "src": "/QuanSlim/stylesheet.css",
      "dest": "/server.js"
    },
    {
      "src": "/QuanSlim/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/Fonts/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
